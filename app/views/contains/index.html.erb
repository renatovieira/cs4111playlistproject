<div class="page-header">
  <%= link_to new_contain_path, class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span>
    New Contain
  <% end %>
  <h1>Listing contains</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
            <th>Playlist</th>
            <th>Song</th>
            <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @contains) do |contain| %>
            <td><%= contain.playlist_id %></td>
            <td><%= contain.song_id %></td>
            <td><%= link_to 'Show', contain %></td>
        <td><%= link_to 'Edit', edit_contain_path(contain) %></td>
        <td><%= link_to 'Destroy', contain, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <% end %>
    </tbody>
  </table>
</div>
